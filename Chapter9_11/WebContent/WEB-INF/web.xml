<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Chapter9_11</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <jsp-config>
  <!-- Before JSP 2.0 you must specify where to look of tlds, you can do as well now
  if you not agree with Container default search location -->
   <taglib>
   		<taglib-uri>randomThings</taglib-uri>
   		<taglib-location>/WEB-INF/tlds/MyFirstTLD.tld</taglib-location>
   </taglib>
   
  	<jsp-property-group>
  		<url-pattern>*.jsp</url-pattern>
  		<el-ignored>false</el-ignored>
  		<scripting-invalid>false</scripting-invalid>
  	</jsp-property-group>
  	
  </jsp-config>
</web-app>